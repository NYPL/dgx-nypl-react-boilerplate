{
  "name": "dgx-nypl-react-boilerplate",
  "version": "0.3.0",
  "description": "Isomorphic React boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "start:dev": "node index && webpack-dev-server --hot --config webpack.common.js",
    "dist": "NODE_ENV=production webpack -p --progress --display-modules --config webpack.prod.config",
    "postinstall": "npm run dist",
    "test": "mocha --compilers js:babel-core/register",
    "test-file": "node_modules/.bin/mocha -w test/helpers/browser.js",
    "test-with-coverage": "./node_modules/.bin/nyc ./node_modules/.bin/mocha test/helpers/browser.js test/*.test.js",
    "coverage": "BABEL_ENV=test node_modules/.bin/nyc --require mocha npm test",
    "coverage-report": "BABEL_ENV=test node_modules/.bin/nyc report --reporter=lcov",
    "coverage-report-html": "BABEL_ENV=test ./node_modules/.bin/nyc report --reporter=html && open -a 'Google Chrome' ./coverage/index.html",
    "init": "sudo rm -rf .git && git init && npm init"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "sass",
    "hot",
    "reload"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com//issues"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "clean-webpack-plugin": "2.0.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "esdoc": "1.1.0",
    "eslint": "5.10.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "2.1.2",
    "jsdom": "13.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "react-hot-loader": "4.6.0",
    "react-test-renderer": "15.6.2",
    "sinon": "7.3.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.4",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "@nypl/design-toolkit": "0.1.36",
    "@nypl/dgx-header-component": "2.5.6",
    "@nypl/dgx-react-footer": "0.5.2",
    "alt": "0.18.6",
    "axios": "0.18.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-istanbul": "5.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "colors": "1.3.3",
    "compression": "1.7.3",
    "css-loader": "2.1.1",
    "dgx-feature-flags": "git+https://bitbucket.org/NYPL/dgx-feature-flags.git#master",
    "dgx-model-data": "git+https://bitbucket.org/NYPL/dgx-model-data.git#master",
    "dgx-react-ga": "git+https://bitbucket.org/NYPL/dgx-react-ga.git#master",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "iso": "5.2.0",
    "node-sass": "4.11.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rootpath": "0.1.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2"
  },
  "directories": {
    "test": "test"
  },
  "author": ""
}
